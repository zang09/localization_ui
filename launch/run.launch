<launch>
  <arg name="project" default="localization_ui"/>
  
  <!--- Sim Time -->
  <param name="use_sim_time" value="false" />
  
  <!--- Start Node -->
  <node pkg="$(arg project)" type="$(arg project)" name="$(arg project)" output="screen" respawn="true">
      <rosparam param="map_origin">[37.56210566421452, 126.9344763024742, 36.20190576370855]</rosparam>
  </node>
    
</launch>
